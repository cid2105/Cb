<-Testing integer arithmetic->
print( 1 + 2 );
print( " = 3" );
print( 1 - 2 );
print( " = -1" );
print( 2 * 2 );
print( " = 4" );
print( 1 / 2 );
print( " = 0" );
print( 5 % 2 );
print( " = 1" );

<-Order of operations with integer arithmetic->
print( 1 + 2 * 3 + 4 );
print( " = 11" );
print( ( 1 + 2 ) * ( 3 + 4 ) );
print( " = 21" );